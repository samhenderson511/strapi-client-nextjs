{
  "name": "strapi-client-nextjs",
  "author": "Sam Henderson",
  "description": "Next.js 14 client for Strapi Rest API using next/cache",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/strapi-client.js",
  "module": "dist/strapi-client.mjs",
  "typings": "dist/strapi-client.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "nodemon --config nodemon.json src/example/play.ts",
    "lint": "eslint  . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "semantic-release": "semantic-release --branches main",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint"
    }
  },
  "peerDependencies": {
    "axios": "^1",
    "next": "^14",
    "qs": "^6"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/qs": "^6.9.15",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "ansi-regex": "^6.0.1",
    "axios": "^1.7.2",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.21.5",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "next": "^14.2.4",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "qs": "6.12.1",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "semantic-release": "^24.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kmariappan/strapi-client-js.git"
  },
  "packageManager": "yarn@4.3.1"
}
